@using Common;
@using Models;
@model MaterielRecord
<div class="control-group"><label class="control-label">ID</label><div class="controls"><input id="ID" name ="ID" type="text" value ="@Model.ID" required readonly="readonly" /></div></div>
<div class="control-group"><label class="control-label">MaterielID</label><div class="controls"><input id="MaterielID" name ="MaterielID" type="text" value ="@Model.MaterielID" style="width:30px;" required/></div></div>
<div class="control-group"><label class="control-label">Stock</label><div class="controls"><input id="Stock" name ="Stock" type="text" value ="@Model.Stock" style="width:30px;" required/></div></div>
<div class="control-group"><label class="control-label">Type</label><div class="controls"><input id="Type" name ="Type" type="text" value ="@Model.Type" style="width:30px;" required/></div></div>
<div class="control-group"><label class="control-label">RequestNo</label><div class="controls"><textarea id="RequestNo" name ="RequestNo">@Model.RequestNo</textarea></div></div>
<div class="control-group"><label class="control-label">Summary</label><div class="controls"><textarea id="Summary" name ="Summary">@Model.Summary</textarea></div></div>
