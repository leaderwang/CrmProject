@using MvcPager;
@using Lib;
@model PagedList<Models.Menu>
<table class="table table-striped">
    <thead>
        <tr>
            <th class="checkbox-column">
                <input type="checkbox" name="checkall" onchange="selectall(this,'ID')" />
            </th>
            <th>
                #
            </th>
            <th>
                名称
            </th>
            <th>
                链接地址
