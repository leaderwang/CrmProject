@using Common;
@using Models;
@model MaterielRequest
<div class="control-group"><label class="control-label">ID</label><div class="controls"><input id="ID" name ="ID" type="text" value ="@Model.ID" required readonly="readonly" /></div></div>
<div class="control-group"><label class="control-label">SchoolID</label><div class="controls"><input id="SchoolID" name ="SchoolID" type="text" value ="@Model.SchoolID" style="width:30px;" required/></div></div>
<div class="control-group"><label class="control-label">申请编号</label><div class="controls"><input id="RequestNo" name ="RequestNo" type="text" value ="@Model.RequestNo" required /></div></div>
<div class="control-group"><label class="control-label">申请标题</label><div class="controls"><input id="Title" name ="Title" type="text" value ="@Model.Title" required /></div></div>
<div class="control-group"><label class="control-label">内容概述</label><div class="controls"><textarea id="Summary" name ="Summary">@Model.Summary</textarea></div></div>
<div class="control-group"><label class="control-label">状态</label><div class="controls"><input id="Status" name ="Status" type="text" value ="@Model.Status" style="width:30px;" required/></div></div>
