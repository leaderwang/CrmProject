@model Models.Course
<div class="control-group"><label class="control-label">教育机构ID</label><div class="controls padt5">@Model.CompanyID</div></div>
<div class="control-group"><label class="control-label">课程编码</label><div class="controls padt5">@Model.CourseNo</div></div>
<div class="control-group"><label class="control-label">课程名称</label><div class="controls padt5">@Model.Title</div></div>
<div class="control-group"><label class="control-label">课程简介</label><div class="controls padt5">@Model.Summary</div></div>
<div class="control-group"><label class="control-label">课程内容</label><div class="controls padt5">@Model.Content</div></div>
<div class="control-group"><label class="control-label">状态</label><div class="controls padt5">@Model.Status</div></div>
<div class="control-group"><label class="control-label">授课老师ID</label><div class="controls padt5">@Model.TeacherID</div></div>
